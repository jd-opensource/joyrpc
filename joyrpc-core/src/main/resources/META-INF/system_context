#系统内置的上下文
#支持引用外部环境变量，或进行重命名。如[key]=[value],表示从环境变量获取"value"的值
#
#注册中心
[registry.protocol]
[registry.address]
#启动属性
[host.cpucores]
[host.memory]
[pid]
[start.time]
[os.type]
#应用信息
[appPath]=[appPath]
[appId]=[appId]
[appName]=[appName]
[appInsId]=[appInsId]
[appGroup]=[appGroup]
#全局的应用服务名称
[appService]=[appService]

